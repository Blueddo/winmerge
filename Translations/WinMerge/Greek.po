msgid ""
"Replace text with another text.\r\n"
"Usage: Replace [-i] [-e] FIND REPLACE\r\n"
"  FIND    - text to find\r\n"
"  REPLACE - text to replace\r\n"
"  -i - ignore case (only for -e)\r\n"
"  -e - treat the specified text as a regular expression"
msgstr ""
"Αντικατάσταση κειμένου με άλλο κείμενο.\r\n"
"Χρήση: Αντικατάσταση [-i] [-e] FIND REPLACE\r\n"
"  FIND - κείμενο στο find\r\n"
"  REPLACE - κείμενο προς αντικατάσταση\r\n"
"  -i - αγνόηση πεζών-κεφαλαίων (μόνο για -e)\r\n"
"  -e - μεταχειρίζεται το καθορισμένο κείμενο ως κανονική έκφραση"

#, c-format
msgid ""
"Apply filter command. \r\n"
"Usage: ExecFilterCommand COMMAND\r\n"
"  COMMAND - command to execute. %1 in the command is replaced with the "
"filename."
msgstr ""
"Εντολή εφαρμογής φίλτρου. \r\n"
"Χρήση: ExecFilterCommand COMMAND\r\n"
"  COMMAND - εντολή προς εκτέλεση. Το %1 στην εντολή αντικαθίσταται με το "
"όνομα αρχείου."

msgid ""
"Tokenize selection. \r\n"
"Usage: Tokenize PATTERNS\r\n"
"  PATTERNS - regular expression for tokenizing. (e.g. [^\\w]+)"
msgstr ""
"Επιλογή tokenize . \r\n"
"Χρήση: Tokenize PATTERNS\r\n"
"  PATTERNS - κανονική έκφραση για την κωδικοποίηση συμβόλων. (π.χ. [^\\w]+)"

msgid "Trim spaces"
msgstr "Εισαγωγή κενών διαστημάτων"

msgid ""
"Select some columns.\r\n"
"Usage: SelectColumns RANGES\r\n"
"   or: SelectColumns [-v] [-i] [-g] -e PATTERNS\r\n"
"  RANGES   - list of column ranges to select. (e.g. -3,5-10,30-)\r\n"
"  PATTERNS - regular expression\r\n"
"  -v - select non-matching columns\r\n"
"  -i - ignore case\r\n"
"  -g - enable global flag\r\n"
"  -e - use PATTERNS for matching"
msgstr ""
"Επιλέξτε ορισμένες στήλες.\r\n"
"Χρήση: SelectColumns RANGES\r\n"
"   ή: SelectColumns [-v] [-i] [-g] -e ΜΟΤΊΒΑ\r\n"
"  RANGES   - λίστα με τις περιοχές των στηλών προς επιλογή. (π.χ. "
"-3,5-10,30-)\r\n"
"  PATTERNS - κανονική έκφραση\r\n"
"  -v - επιλογή μη ταιριαστών στηλών\r\n"
"  -i - αγνόηση πεζών-κεφαλαίων\r\n"
"  -g - ενεργοποίηση καθολικής σημαίας\r\n"
"  -e - χρήση PATTERNS για αντιστοίχιση"

msgid ""
"Select some lines.\r\n"
"Usage: SelectLines RANGES\r\n"
"   or: SelectLines [-v] [-i] -e PATTERNS\r\n"
"  RANGES   - list of line ranges to select. (e.g. -3,5-10,30-)\r\n"
"  PATTERNS - regular expression\r\n"
"  -v - select non-matching lines\r\n"
"  -i - ignore case\r\n"
"  -e - use PATTERNS for matching"
msgstr ""
"Επιλέξτε μερικές γραμμές.\r\n"
"Χρήση: SelectLines RANGES\r\n"
"   ή: SelectLines [-v] [-i] -e PATTERNS\r\n"
"  RANGES   - λίστα με τις περιοχές γραμμών που μπορείτε να επιλέξετε. (π.χ. "
"-3,5-10,30-)\r\n"
"  PATTERNS - κανονική έκφραση\r\n"
"  -v - επιλογή μη ταιριαστών γραμμών\r\n"
"  -i - αγνόηση πεζών-κεφαλαίων\r\n"
"  -e - χρήση PATTERNS για την αντιστοίχιση"

